def build(path, ci_image) {
    stage("Build") {
        docker.image("egovio/ci-react:0.0.3").inside {
            sh "cp -a /tmp/node_modules ${path}/ && cd ${path} && npm set registry http://egov-micro-dev.egovernments.org/verdaccio && npm install && npm run build";
        }
    }
}

return this;
